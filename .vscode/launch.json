    "version": "0.2.0",
    "configurations": [


        {
            "name": "Launch cel grpc only service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}",
            "program": "./cmd/grpc/main.go"
        },
        {
            "name": "Launch cel service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}",
            "program": "./cmd/service/main.go",
            "args": [
                "-c", 
                "./configs/service_local.yaml",
                "--grpctsl",
                "true",
            ]
        },
        {
            "name": "Launch cel cli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "cwd": "${workspaceFolder}",
            "program": "./cmd/cli/main.go"
        }
    ]
}